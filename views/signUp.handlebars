<div class="login-signup" id="form-signin">
  {{! <h3>Sign Up</h3> }}
  <form id="signUpForm">

    <div class="form-group">
      <label for="username" class="text-color">Username</label>
      <div class="username">
        <input id="nameSignup" type="text" placeholder="username" />
      </div>
    </div>
    <div class="form-group">
      <label for="password" class="text-color">Password</label>
      <div class="password">
        <input id="passwordSignup" type="password" placeholder="password" />
      </div>
    </div>
    <div class="btn-group">
      <button type="submit" id="btn-sign" class="btn btn-color">Sign Up</button>
      <a href="/login"> Or Login Here </a>
    </div>
  </form>
</div>

<script src="/javaScript/signup.js"></script>